@model DogAPI.Areas.Admin.Models.DogModel
@{
    ViewBag.Title = "Create Dog";
}

@using (Html.BeginForm("Index", "Dog", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="content-wrapper">
        <section class="content-header">
            <h1 align="left" class="float-lg-right">
                Create Dog
            </h1>
            <h3 align="right" style="margin:1%;"> @Html.ActionLink("DogList", "DogList", "Dog")</h3>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="box box-primary">
                @{ Html.RenderPartial("_CreateOrUpdatePartial", Model);}
                <!-- /.row -->
            </div>
        </section>
        <!-- /.content -->
    </div>
}
